@model Libellus.Web.Areas.Administracion.Models.UsuariosAdministrativos.UsuarioAdministrativoViewModel

@{
    ViewBag.Title = "Crear usuarios administrativos";
}

<link type="text/css" rel="stylesheet" href="@Url.Content("~/Content/DateTimePicker/bootstrap-datetimepicker.css")" />

<div class="content-header">
    <h2 class="content-header-title">@ViewBag.Title</h2>
</div>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    Html.RenderPartial("_Administrar");

    <br />

    <div class="form-group centered">
        @Html.BotonCancelar("Consultar", "UsuariosAdministrativos", new { tabindex = "16" })
        <input type="submit" value="Crear usuario" class="btn btn-primary" tabindex="17" />
    </div>

}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/js/datetimepicker")
}