@model Libellus.Web.Areas.GestionAcademica.Models.AspectoEvaluativo.AspectoEvaluativoViewModel

@{
    ViewBag.Title = "Editar aspectos evaluativos";
}

<div class="content-header">
    <h2 class="content-header-title">@ViewBag.Title</h2>
</div>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    Html.RenderPartial("_Administrar");

    <br />

    <div class="form-group centered">
        @Html.BotonCancelar("Consultar", "AspectoEvaluativo", new { tabindex = "7" })
        <input type="submit" value="Editar aspectos evaluativos" class="btn btn-primary" tabindex="8" id="submitAdministrar" />
    </div>
}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
